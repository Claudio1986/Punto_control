<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="cl.puntocontrol.hibernate.domain.Producto" table="producto">
    <id name="prd_codigo" type="java.lang.String">
        <column 							name="PRD_CODIGO" />
    </id>
    <property name="prd_descripcion" 	column="PRD_DESCRIPCION" />
    <property name="codigo_especie" 	column="codigo_especie" />
    
  </class>
</hibernate-mapping>